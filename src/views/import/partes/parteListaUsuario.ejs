

        <table border="3" class="table table-striped  table-dark col-md-8 text-center centrade">
            <thead class= "">
                <tr>
                    <th>id</th>
                    <th>Nombre de usuario</th>
                    <th>Nombre</th>
                    <th>Apellido</th>
                    <th>Bloqueado</th>
                    <th>Usuario</th>
                </tr>
            </thead>

            <% for(i=0; i < usuarios.length; i++){ %>
            <tbody>
                    <tr>
                    <th ><%= usuarios[i].id %></th>
                    <th ><%= usuarios[i].nombre_usuario %></th>
                    <th ><%= usuarios[i].nombre %></th>
                    <th ><%= usuarios[i].apellido %></th>

                    <% if (usuarios[i].bloqueado){%>
                    <th > <input type="checkbox" name="" checked="checked" > </th>

                    <%} else{ %>
                    <th > <input type="checkbox" name="" > </th>
                    <% } %>
                    <% if (usuarios[i].supervisor ){%>
                      <th > supervisor </th>

                      <%} else{ %>
                      <th > operador </th>
                        <% } %>

                    </tr>
            </tbody>
            <% } %>

        </table>

    </div>
</div>
