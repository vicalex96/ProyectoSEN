
<div class="container">
	<div class="row">
	<table id="table" class="table table-striped table-bordered table-dark" style="width:100%">
        <thead>
            <tr>
                <th><input type="checkbox" onclick="checkAll(this)"></th>
                <th>Name</th>
                <th>Position</th>
                <th>Office</th>
                <th>Age</th>
                <th>Start date</th>
                <th>Salary</th>
            </tr>
        </thead>
        <tbody>
			<% for(i=0; i < nodos.length; i++){ %>
                <tr>
                    <th ><%= nodos[i].id %></th>
                    <th ><%= nodos[i].nombre %></th>
                    <th ><%= nodos[i].latitud %></th>
                    <th ><%= nodos[i].longitud %></th>
                    <th ><%= nodos[i].tipo_nodo %></th>
                    <th ><input type="button" value= "editar" class="btn btn-success"></th>
                    <th ><input type="button" value= "eliminar" class=" btn btn-danger"></th>
                </tr>
            <% } %>
		</tbody>
    </table>
	</div>
</div>
