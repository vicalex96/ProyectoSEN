<!DOCTYPE html> <!--- View Registrar Usuario -->
<html lang="en" dir="ltr">
  <head>
        <% include import/viewhtmlHeader.ejs %>
        <link rel="stylesheet" href="css/standardStile.css">
        <link rel="stylesheet" href="css/registerStile.css">

  </head>
  <body>
    <body>
        <% include import/viewHeader.ejs %>

        <div class=" container centrade color-white">
        	<div class="row">
        	<table id="table" class="table table-striped table-bordered table-dark" style="width:100%">
                <thead>
                    <tr>
                        <th>id</th>
                        <th>fecha</th>
                        <th>accion</th>
                        <th>elemento afectado</th>
                        <th>nombre operador</th>
                    </tr>
                </thead>
                <tbody>
                    <% for(i=0; i < acciones.length; i++){ %>
                            <tr>
                            <th ><%= acciones[i].id %></th>
                            <th ><%= acciones[i].fecha %></th>
                            <th ><%= acciones[i].tipo_accion %></th>
                            <th ><%= acciones[i].objeto_afectado %></th>
                            <th ><%= acciones[i].nombre_operador %></th>
                            </tr>
                    <% } %>
        		</tbody>
            </table>
        	</div>
        </div>
        <script src="../js/paginaTabla.js"></script>
        <% include import/BSConnect.ejs %>

    </body>
</html>
